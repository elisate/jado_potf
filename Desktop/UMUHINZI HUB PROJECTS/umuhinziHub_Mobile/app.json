{
  "expo": {
    "name": "umuhinziHub",
    "slug": "umuhinziHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://res.cloudinary.com/dc6iwekzx/image/upload/v1762504770/upscalemedia-transformed-removebg-preview_ltzhkr.png",
    "scheme": "umuhinzihub",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.b16.umuhinziHub"
    },
    "web": {
      "output": "static",
      "favicon": "https://res.cloudinary.com/dc6iwekzx/image/upload/v1762504770/upscalemedia-transformed-removebg-preview_ltzhkr.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "https://res.cloudinary.com/dc6iwekzx/image/upload/v1762504770/upscalemedia-transformed-removebg-preview_ltzhkr.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5fdb12b5-dc44-4762-9c14-5923daafbdcc"
      }
    }
  }
}
